{
  "name": "@mi-zon/bot",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "pnpm start --watch",
    "start:debug": "pnpm start:dev --debug",
    "start:prod": "node dist/main.js",
    "build": "nest build",
    "lint": "eslint --ignore-path ../../.gitignore --cache --cache-location ./.cache/eslint/ .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "docs": "pnpm compodoc -p tsconfig.json -d ./docs",
    "docs:serve": "pnpm run docs -s",
    "test": "node --expose-gc ../../node_modules/jest/bin/jest -c jest.config.ts",
    "test:coverage": "pnpm test -- --coverage",
    "test:watch": "pnpm test -- --watch",
    "test:debug": "node --inspect-brk --expose-gc ../../node_modules/jest/bin/jest -c jest.config.ts --runInBand",
    "env:copy": "cp .env.example .env",
    "typeorm": "tsx ./node_modules/typeorm/cli",
    "migration:create": "pnpm typeorm migration:create",
    "migration:generate": "pnpm typeorm migration:generate -d typeorm.config.ts",
    "migrate": "pnpm typeorm migration:run -d typeorm.config.ts",
    "migrate:rollback": "pnpm typeorm migration:revert -d typeorm.config.ts",
    "migrate:rollback:all": "pnpm typeorm schema:drop -d typeorm.config.ts",
    "migrate:fresh": "pnpm migrate:rollback:all && pnpm migrate",
    "start:repl": "pnpm start --watch --entryFile repl",
    "ngrok": "ngrok http 3000",
    "import:v2-data": "tsx scripts/import-v2-data.mts",
    "import:v2-verification": "tsx scripts/import-v2-verifications-data.mts",
    "import:v2-user-limits": "tsx scripts/import-v2-users-limit.mts",
    "import:v2-note": "tsx scripts/import-v2-notes-data.mts",
    "v2:fix-users-location-data": "tsx scripts/fix-users-location-data.mts",
    "import:v2-order": "tsx scripts/import-v2-orders-data.mts",
    "fill-transactions-usd-rate": "tsx scripts/fill-transaction-usd-conversion-rate.mts"
  },
  "imports": {
    "#src/*": "./dist/*"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.329.0",
    "@nestjs/axios": "2.0.0",
    "@nestjs/bull": "0.6.3",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "2.3.4",
    "@nestjs/core": "9.4.3",
    "@nestjs/event-emitter": "2.0.4",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/microservices": "9.4.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/schedule": "2.2.3",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/throttler": "5.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@nestjsx/crud": "5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "5.0.0-alpha.3",
    "@sendgrid/mail": "7.7.0",
    "@slack/web-api": "6.10.0",
    "ajv": "8.17.1",
    "axios": "1.4.0",
    "bignumber.js": "9.1.2",
    "bull": "4.10.4",
    "cache-manager": "5.2.1",
    "cache-manager-redis-yet": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "class-validator-extended": "4.0.0",
    "cookie-parser": "1.4.6",
    "csv": "6.3.9",
    "csv-stringify": "6.4.3",
    "dotenv": "16.3.1",
    "google-auth-library": "8.8.0",
    "handlebars": "4.7.7",
    "helmet": "7.0.0",
    "ioredis": "^5.4.1",
    "ip": "1.1.8",
    "lodash": "4.17.21",
    "mime-types": "2.1.35",
    "moment": "2.29.4",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "multer": "1.4.5-lts.1",
    "nest-winston": "1.9.2",
    "nestjs-fifo-lock": "^1.0.3",
    "nestjs-throttler-storage-redis": "0.4.4",
    "nodemailer": "6.9.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pg": "8.11.5",
    "puppeteer": "19.4.0",
    "pusher": "5.1.3",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "twilio": "5.0.4",
    "typeorm": "0.3.20",
    "uuid": "9.0.1",
    "winston": "3.8.2",
    "xrpl": "3.0.0",
    "libphonenumber-js": "1.11.1",
    "mezon-sdk": "^2.7.68"
  },
  "devDependencies": {
    "@nestjs/testing": "9.4.3"
  }
}
